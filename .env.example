# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/absolutsport_forms

# JWT
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRES_IN=7d

# Server
PORT=3333
NODE_ENV=development

# Google Sheets
SHEETS_SPREADSHEET_ID=your-spreadsheet-id
GOOGLE_SERVICE_ACCOUNT_JSON=

# Supabase (Storage for profile photos)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key
SUPABASE_BUCKET=profile-photos

# Resend (Email)
RESEND_API_KEY=your-resend-api-key
RESEND_FROM=AbsolutSport Forms <noreply@absolut-sport.com.br>

# Frontend URL (for CORS and email links)
FRONTEND_URL=http://localhost:5173
